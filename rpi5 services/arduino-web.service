[Unit]
Description=Arduino Web UI
After=network.target

[Service]
WorkingDirectory=/workspaces/Cristi_RPI5-arduino-http/rpi5
ExecStart=/usr/bin/python3 /workspaces/Cristi_RPI5-arduino-http/rpi5/web_server.py
Restart=always
User=pi
Environment=FLASK_SECRET_KEY=changeme

[Install]
WantedBy=multi-user.target
